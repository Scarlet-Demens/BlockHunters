{
    "schemaVersion": 1,
    "id": "blockhunters",
    "version": "${version}",
    "name": "Block Hunters",
    "description": "",
    "authors": [
        "Scarlet_Demens"
    ],
    "contact": {
        "issues": "https://github.com/Scarlet-Demens/BlockHunters/issues",
        "sources": "https://github.com/Scarlet-Demens/BlockHunters"
    },
    "license": "MIT License",
    "icon": "assets/blockhunters/icon.png",
    "environment": "*",
    "accessWidener" : "blockhunters.accesswidener",
    "custom": {
        "loom:injected_interfaces": {
            "net/minecraft/class_1657": ["org/demens/blockhunters/extension/BlockDisguise"]
        },
        "mc-publish": {
            "dependencies": [
                "owo-lib(required)"
            ]
        }
    },
    "entrypoints": {
        "client": [
            "org.demens.blockhunters.client.BlockHuntersClient"
        ],
        "main": [
            "org.demens.blockhunters.BlockHunters"
        ]
    },
    "mixins": [
        "blockhunters.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${loader_version}",
        "fabric": "*",
        "minecraft": "${minecraft_version}",
        "owo": ">=${owo_version}"
    }
}
